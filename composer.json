{
	"name": "leonis/easysms-notification-channel",
	"description": "EasySms Notification Channel for Laravel.",
	"keywords": ["laravel", "notifications", "easysms"],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "yangliu",
			"email": "yangliulnn@163.com"
		}
	],
	"require": {
		"overtrue/easy-sms": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Leonis\\Notifications\\EasySms\\": "src"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
